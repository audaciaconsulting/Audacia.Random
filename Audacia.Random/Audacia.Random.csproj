<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup Label="Globals">
    <SccProjectName>SAK</SccProjectName>
    <SccProvider>SAK</SccProvider>
    <SccAuxPath>SAK</SccAuxPath>
    <SccLocalPath>SAK</SccLocalPath>
    <TargetFrameworks>netstandard1.2;netstandard2.0</TargetFrameworks>
    <RepositoryUrl>https://github.com/audaciaconsulting/Audacia.Random</RepositoryUrl>
  </PropertyGroup>
  <PropertyGroup>
    <AssemblyVersion>4.0.0.0</AssemblyVersion>
    <Version>4.0.0.0</Version>
    <Authors>Adam Drewery</Authors>
    <Company>Audacia</Company>
    <PackageIconUrl>https://www.audacia.co.uk/media/pkenoobu/audacia-logo-circle-blue.png</PackageIconUrl>
    <Description>Utilities and extension methods for generating random organic test data.</Description>
    <Copyright>Copyright © Audacia 2017</Copyright>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="companies" />
    <None Remove="forenames.female" />
    <None Remove="forenames.male" />
    <None Remove="Sentences" />
    <None Remove="streets" />
    <None Remove="surnames" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="Cities" />
    <EmbeddedResource Include="Companies" />
    <EmbeddedResource Include="Counties" />
    <EmbeddedResource Include="Forenames.Female" />
    <EmbeddedResource Include="Forenames.Male" />
    <EmbeddedResource Include="Sentences" />
    <EmbeddedResource Include="Streets" />
    <EmbeddedResource Include="Surnames" />
    <EmbeddedResource Include="Nouns" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="Properties\" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.CodeAnalysis.FxCopAnalyzers" Version="2.9.4" />
  </ItemGroup>
</Project>